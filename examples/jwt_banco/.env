# Configurações do servidor
PORT=8080
HOST=localhost

# Configurações JWT
JWT_SECRET_KEY=sua-chave-secreta-muito-segura-aqui
JWT_ISSUER=go-data-server
JWT_EXPIRES_IN=24h
JWT_REFRESH_IN=168h

# Configurações do banco de dados
DB_PATH=jwt_example.db
DB_DRIVER=sqlite3

# Configurações de autenticação
ENABLE_JWT=true
REQUIRE_AUTH=true

# Logs
LOG_LEVEL=info
